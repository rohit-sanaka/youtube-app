{
  "semi": true,
  "tabWidth": 2,
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma":"none",
  "jsxBracketSameLine": false,
  "rules": {
  "prettier/prettier": [
    "error",
    {
      "endOfLine": "auto",
      "singleQuote": true,
      "parser": "flow"
    },
    {
      "usePrettierrc": false
    },
    {
      "fileInfoOptions": {
        "withNodeModules": true
      }
    }
  ]
}
}

